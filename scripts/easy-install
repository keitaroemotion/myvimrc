cp .vimrc ~
mkdir -p $HOME/.vim/scripts
cp scripts/* $HOME/.vim/scripts
cp aliases/* /usr/local/bin
cp scripts/ins /usr/local/bin
cp ./scripts/tocamel /usr/local/bin
